        "globals": "^15.8.0",
        "prettier": "3.3.3",
        "sass": "^1.77.8",
        "vite": "^5.3.5",
        "vite-plugin-handlebars": "^2.0.0",
        "vite-plugin-live-reload": "^3.0.3",
        "vite-plugin-pwa": "^0.20.1",
        "vitest": "^2.0.5"
    },
    "dependencies": {
        "@alpinejs/persist": "^3.14.1",
